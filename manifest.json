{
  "manifest_version": 3,
  "name": "TabMaster",
  "description": "chrome tab list for easy",
  "version": "1.1.1",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}
